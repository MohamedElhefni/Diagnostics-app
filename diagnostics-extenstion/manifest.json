{
  "name": "Diagnostics Extenstion",
  "version": "1.0",
  "description": "Build an Extension to get System Information ",
  "permissions": ["system.cpu","system.memory","system.storage","system.display","activeTab", "declarativeContent", "storage", "<all_urls>", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts":[ {
    "matches": ["http://127.0.0.1:5500/"],
    "js":[ "moment.js","chart.js", "chartjs-streaming.min.js","content.js"]
  }],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/icon16.png",
      "32": "img/icon32.png",
      "48": "img/icon48.png",
      "128": "img/icon128.png"
    }
  },
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "manifest_version": 2
}
