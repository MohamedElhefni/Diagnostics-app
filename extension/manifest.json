{
  "name": "Diagnostic Extension",
  "description": "Diagnostic Extension that sends system info to the pwa ",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "system.cpu",
    "system.display",
    "system.memory",
    "system.storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
    "matches": ["*://diagnostics-app.netlify.app/*", "*://diagnostics.com/*"]
  }
}
